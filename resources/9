                                        // lazyloading
            googletag.pubads().enableLazyLoad({
                fetchMarginPercent: 200,  // fetch slots within 2 viewports
                renderMarginPercent: 100,  // render slots within 1 viewport
                mobileScaling: 1.2  // increase above values on mobile by 1.2x
            });
            // single request architecture
            googletag.pubads().disableInitialLoad();
            googletag.pubads().enableSingleRequest();
            googletag.enableServices();
            // refresh refreshable ads after x seconds after they have been viewable
            googletag.pubads().addEventListener('impressionViewable', function(event) {
                window.GPTadSlots.refreshableSlots.forEach(function(sl) {
                    if(sl.slot.getSlotElementId() != event.slot.getSlotElementId()) {
                        return;
                    }
                    setTimeout(function() {
                        apstag.fetchBids({
                            slots: [{
                                slotID: sl.slotID,
                                slotName: sl.slotName,
                                sizes: sl.sizes
                            }]
                        }, function(bids) {
                            // set apstag bids, then trigger the 2nd-nth request to DFP
                            apstag.setDisplayBids();
                            googletag.pubads().refresh([sl.slot]);
                        });
                    }, sl.seconds*1000);
                })
            });
        });
