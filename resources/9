            beforeSend: function(jqXHR, settings) {
                var bindSymbol = '?';
                if(settings.url.search(/\?/) >= 0) {
                    bindSymbol = '&';
                }
                if(settings.url.match(/krone\.at/)) {
                  settings.url = settings.url + bindSymbol + 'cors=true&domain=' + window.location.host;
                }
            }
