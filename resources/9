  <script>
        window.KRN_CORE_CONFIG = {
            redisToken: '3dd3f74b30a1819c4a51ca5ddf2d6ee32a0e585b',
            sentry: {
                dsn: 'https://bbe926f4591b4e02a80f70afd62334f2@sentry.krone.at/4',
